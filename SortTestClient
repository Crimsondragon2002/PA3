
public class SortTestClient {
	public static  void MergeSortTest()
	{
		int array[] = { 8, 6, 7, 5, 3, 0, 9, 99, 44, 100, 11 };
		MergeSort merging = new MergeSort();
		Helper help = new Helper();
        System.out.print("Given array is ");
        help.printArray(array);
 
        
        merging.sortMerge(array, 0, array.length - 1);
 
        System.out.print("\nSorted array is ");
        help.printArray(array);
        System.out.println("");
        int[] arraymr1 = help.getMoreRandomArray(5);
        int[] arraymr2 = help.getMoreRandomArray(7);
        int[] arraymr3 = help.getMoreRandomArray(9);
        int[] arraylr1 = help.getLessRandomArray(5);
        int[] arraylr2 = help.getLessRandomArray(7);
        int[] arraylr3 = help.getLessRandomArray(9);
        int[] arraya1 = help.getAscendingArray(5);
        int[] arraya2 = help.getAscendingArray(7);
        int[] arraya3 = help.getAscendingArray(9);
        int[] arrayd1 = help.getDescendingArray(5);
        int[] arrayd2 = help.getDescendingArray(7);
        int[] arrayd3 = help.getDescendingArray(9);
      
        System.out.print("Array more random 1 is ");
        help.printArray(arraymr1);
        
        System.out.println("");
        
        System.out.print("When Sorted it is is ");
        merging.sortMerge(arraymr1, 0, arraymr1.length-1);
        help.printArray(arraymr1);
        System.out.println("");
        System.out.print("Array more random 2 is ");
        help.printArray(arraymr2);
        System.out.println("");
        System.out.print("When Sorted it is is ");
        merging.sortMerge(arraymr2, 0, arraymr2.length-1);
        help.printArray(arraymr2);
        System.out.println("");
        System.out.print("Array more random 3 is ");
        help.printArray(arraymr3);
        System.out.println("");
        System.out.print("When Sorted it is is ");
        merging.sortMerge(arraymr3, 0, arraymr3.length-1);
        help.printArray(arraymr3);
        System.out.println("");
        System.out.print("Array less random 1 is ");
        help.printArray(arraylr1);
        System.out.println("");
        System.out.print("When Sorted it is is ");
        merging.sortMerge(arraylr1, 0, arraylr1.length-1);
        help.printArray(arraylr1);
        System.out.println("");
        System.out.print("Array less random 2 is ");
        help.printArray(arraylr2);
        System.out.println("");
        System.out.print("When Sorted it is is ");
        merging.sortMerge(arraylr2, 0, arraylr2.length-1);
        help.printArray(arraylr2);
        System.out.println("");
        System.out.print("Array lr3 is ");
        help.printArray(arraylr3);
        System.out.println("");
        System.out.print("When Sorted it is is ");
        merging.sortMerge(arraylr3, 0, arraylr3.length-1);
        help.printArray(arraylr3);
        System.out.println("");
        System.out.print("Array ascend 1 is ");
        help.printArray(arraya1);
        System.out.println("");
        System.out.print("When Sorted it is is ");
        merging.sortMerge(arraya1, 0, arraya1.length-1);
        help.printArray(arraya1);
        System.out.println("");
        System.out.print("Array ascend 2 is ");
        help.printArray(arraya2);
        System.out.println("");
        System.out.print("When Sorted it is is ");
        merging.sortMerge(arraya2, 0, arraya2.length-1);
        help.printArray(arraya2);
        System.out.println("");
        System.out.print("Array ascend 3 is ");
        help.printArray(arraya3);
        System.out.println("");
        System.out.print("When Sorted it is is ");
        merging.sortMerge(arraya3, 0, arraya3.length-1);
        help.printArray(arraya3);
        System.out.println("");
        System.out.print("Array decend 1 is ");
        help.printArray(arrayd1);
        System.out.println("");
        System.out.print("When Sorted it is is ");
        merging.sortMerge(arrayd1, 0, arrayd1.length-1);
        help.printArray(arrayd1);
        System.out.println("");
        System.out.print("Array decend 2 is ");
        help.printArray(arrayd2);
        System.out.println("");
        System.out.print("When Sorted it is is ");
        merging.sortMerge(arrayd2, 0, arrayd2.length-1);
        help.printArray(arrayd2);
        System.out.println("");
        System.out.print("Array descend 3 is ");
        help.printArray(arrayd3);
        System.out.println("");
        System.out.print("When Sorted it is is ");
        merging.sortMerge(arrayd3, 0, arrayd3.length-1);
        help.printArray(arrayd3);
        System.out.println("");
	}
